{
  "name": "submarine-js",
  "version": "0.4.0-beta.0",
  "description": "Javascript client library for Submarine.",
  "repository": {
    "type": "git",
    "url": "https://github.com/discolabs/submarine-js.git"
  },
  "keywords": [
    "submarine",
    "shopify"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "prepublishOnly": "vite build",
    "serve": "vite preview"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/submarine.umd.js",
  "module": "./dist/submarine.es.js",
  "exports": {
    ".": {
      "import": "./dist/submarine.es.js",
      "require": "./dist/submarine.umd.js"
    }
  },
  "dependencies": {
    "json-api-models": "^0.1.0-beta.7"
  },
  "devDependencies": {
    "js-sha256": "^0.9.0",
    "vite": "^2.9.9"
  }
}
